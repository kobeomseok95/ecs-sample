{
  "family": "ecs-t4g-task-definition",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "300",
  "memory": "300",
  "executionRoleArn": "arn:aws:iam::462433531752:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sample-container",
      "image": "462433531752.dkr.ecr.ap-northeast-2.amazonaws.com/ecs-sample-repository:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
