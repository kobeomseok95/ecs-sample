{
  "family": "ecs-sample-definition",
  "requiresCompatibilities": [
    "EC2"
  ],
  "executionRoleArn": "arn:aws:iam::462433531752:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "cpu": "1024",
  "memory": "300",
  "containerDefinitions": [
    {
      "name": "ecs-sample-container",
      "image": "462433531752.dkr.ecr.ap-northeast-2.amazonaws.com/ecs-sample-repository:latest",
      "memory": 300,
      "memoryReservation": 300,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
